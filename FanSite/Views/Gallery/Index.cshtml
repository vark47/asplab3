@{
    string[] photos = { "images/image6.jpg", "images/image2.jpg", "images/image3.jpg", "images/image4.jpg", "images/image5.jpg", "images/image7.jpg"};
    ViewData["Title"] = "Gallery";
}

<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="/css/styles.css">
</head>

<body>
    <h1>Photos</h1>

    <p>Below are photos chronicling just a few of Austin Rutledge's greatest achievements </p>

    @foreach (string i in photos)
    {
        <img src= @i class="galleryImage" />
    }
   

</body>

</html>